if [ -x "/etc/init.d/cumulocity-agent" ]; then
    invoke-rc.d cumulocity-agent stop || true
    update-rc.d cumulocity-agent remove > /dev/null
fi

if [ -f "/usr/share/cumulocity-rpi-agent/cfg/cumulocity.properties.template" ]; then
    rm /usr/share/cumulocity-rpi-agent/cfg/cumulocity.properties.template
fi
